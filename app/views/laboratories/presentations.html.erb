<% title "Presentations for #{@laboratory.name} Library" %>

<% if current_user.laboratory == @laboratory %>
	<%= render :partial => 'mylabsidenav' %>
<% elsif @laboratory.collaborators.include?(current_user.laboratory) %>
	<%= render :partial => "friendlabsidenav" %>
<% end %>

<div class="fl">

<%= render :partial => 'users' %>

	<h2 style="display:inline;float:left;">
		Presentations for <%= link_to @laboratory.name, @laboratory %> (<%= @laboratory.protocols.count %>)
	</h2>

<% if current_user.laboratory == @laboratory %>
	<%= render :partial => 'mylablibrary' %>
<% elsif @laboratory.collaborators.include?(current_user.laboratory) %>
	<%= render :partial => 'friendlablibrary' %>
<% end %>